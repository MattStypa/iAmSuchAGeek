{
    "manifest_version": 2,

    "name": "I am such a geek",
    "description": "Replaces every @stauffermatt tweet with 'I am such a geek'",
    "version": "1.0",

    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "replace.js"
            ],
            "run_at": "document_end"
        }
    ]

}
